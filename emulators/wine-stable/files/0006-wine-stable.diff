+    {
+        ok(p_byte_reverse_table[i] == byte_reverse(i), "Got unexpected byte %#x, expected %#x.\n",
+                p_byte_reverse_table[i], byte_reverse(i));
+    }
+}
+
 START_TEST(concrt140)
 {
     if (!init()) return;
     test_CurrentContext();
     test_Timer();
+    test_data_exports();
 }

